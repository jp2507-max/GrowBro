{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "production": {
      "channel": "production",
      "distribution": "store",
      "pnpm": "10.15.0",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "EXPO_NO_DOTENV": "1",
        "APP_ENV": "production",
        "FLIPPER_DISABLE": "1",
        "HUSKY": "0",
        "SCHEME": "growbro",
        "EXPO_PUBLIC_SCHEME": "growbro"
      }
    },
    "staging": {
      "channel": "staging",
      "distribution": "internal",
      "pnpm": "10.15.0",
      "ios": {
        "image": "latest"
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "staging",
        "EXPO_NO_DOTENV": "1",
        "FLIPPER_DISABLE": "1",
        "HUSKY": "0",
        "SCHEME": "growbro-staging",
        "EXPO_PUBLIC_SCHEME": "growbro-staging"
      }
    },
    "development": {
      "channel": "preview",
      "developmentClient": true,
      "distribution": "internal",
      "pnpm": "10.15.0",
      "ios": {
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "development",
        "EXPO_NO_DOTENV": "1",
        "HUSKY": "0",
        "BUNDLE_ID": "com.growbro",
        "SCHEME": "growbro-dev",
        "EXPO_PUBLIC_SCHEME": "growbro-dev"
      }
    },
    "simulator": {
      "channel": "preview",
      "pnpm": "10.15.0",
      "ios": {
        "simulator": true,
        "image": "latest"
      },
      "android": {
        "image": "latest"
      },
      "env": {
        "APP_ENV": "development",
        "EXPO_NO_DOTENV": "1",
        "HUSKY": "0",
        "SCHEME": "growbro-dev",
        "EXPO_PUBLIC_SCHEME": "growbro-dev"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
