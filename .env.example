# Example Environment Variables
APP_ENV=development

# API Configuration
API_URL=https://api-dev.example.com
VAR_NUMBER=123
VAR_BOOL=true

# Strains API Configuration
# Direct API access (development only)
STRAINS_API_URL=https://the-weed-db.p.rapidapi.com
STRAINS_API_KEY=your_rapidapi_key_here
STRAINS_API_HOST=the-weed-db.p.rapidapi.com

# Proxy Configuration
# Set to true to use Supabase Edge Function proxy (recommended for production)
STRAINS_USE_PROXY=false

# Feature Flags for Strains
FEATURE_STRAINS_ENABLED=true
FEATURE_STRAINS_FAVORITES_SYNC=true
FEATURE_STRAINS_OFFLINE_CACHE=true

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# OAuth Configuration (optional - only needed for Apple/Google sign in)
APPLE_CLIENT_SECRET=your_apple_jwt_secret_here
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Sentry Configuration
SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0
SENTRY_SEND_DEFAULT_PII=false
SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0.1
SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0
SENTRY_ENABLE_REPLAY=true

# Security & certificate pinning
# FEATURE_SECURITY_CERTIFICATE_PINNING: Enable/disable certificate pinning at the HTTP client level
# FEATURE_SECURITY_BYPASS_PINNING: Bypass pinning checks (only for development/testing)
# SECURITY_PIN_DOMAINS: Comma-separated hostnames (supports .example.com for subdomains)
#   Example: your-app.supabase.co,.your-domain.com
# SECURITY_PIN_HASHES: Comma-separated SHA-256 pin digests matching the domain list
#   Generate using: openssl s_client -connect your-domain.com:443 -servername your-domain.com | openssl x509 -pubkey -noout | openssl pkey -pubin -outform DER | openssl dgst -sha256 -binary | openssl base64
#   Format: sha256/[base64-hash]
#   Example: sha256/AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA=
FEATURE_SECURITY_CERTIFICATE_PINNING=false
FEATURE_SECURITY_BYPASS_PINNING=true
# Comma-separated hostnames (supports .example.com for subdomains)
SECURITY_PIN_DOMAINS=
# Comma-separated SHA-256 pins matching the domains (e.g., sha256/AAAAAAAA...)
SECURITY_PIN_HASHES=

# Build-time variables
SECRET_KEY=replace_me
EXPO_PUBLIC_ACCOUNT_DELETION_URL=https://growbro.app/delete-account
CODE_SIGNING_CERT_PATH=./certs/code-signing.dev.pem
CODE_SIGNING_KEY_ID=growbro-dev
CODE_SIGNING_ALG=rsa-v1_5-sha256

# Community Moderation Feature Flags (DSA Compliance)
# All features default to disabled (ship dark) until compliance validation complete

# SoR Export to DSA Transparency Database (Art. 24(5))
# Enable after: Commission DB API credentials configured, PII scrubbing golden tests passing
FEATURE_SOR_EXPORT_ENABLED=false

# Age Verification and Minor Protection (DSA Art. 28)
# Enable after: DPIA approval, third-party provider contract signed, no-raw-ID verified
FEATURE_AGE_VERIFICATION_ENABLED=false

# Geo-blocking and Regional Content Restrictions
# Enable after: IP geolocation validated (>95%), ePrivacy compliance verified
FEATURE_GEO_BLOCKING_ENABLED=false

# Trusted Flagger Priority Lane (DSA Art. 22)
# Enable after: Certification criteria documented, quality analytics operational
FEATURE_TRUSTED_FLAGGERS_ENABLED=false

# Content Reporting System (DSA Art. 16)
# Enable after: Two-track intake tested, field validation working
FEATURE_CONTENT_REPORTING_ENABLED=false

# Moderation Queue and Decision Workflow (DSA Art. 17)
# Enable after: SoR generation tested, policy catalog integrated
FEATURE_MODERATION_QUEUE_ENABLED=false

# Appeals System (DSA Art. 20 Internal Complaints)
# Enable after: Conflict-of-interest prevention working, human review guaranteed
FEATURE_APPEALS_ENABLED=false

# ODS Integration (DSA Art. 21 Out-of-Court Dispute)
# Enable after: Certified ODS body selected, API integration tested
FEATURE_ODS_INTEGRATION_ENABLED=false

# Repeat Offender Detection (DSA Art. 23 Measures Against Misuse)
# Enable after: Pattern detection tested, graduated enforcement working
FEATURE_REPEAT_OFFENDER_DETECTION_ENABLED=false

# Transparency Reporting (DSA Art. 15 & 24)
# Enable after: Metrics aggregation tested, report generation working
FEATURE_TRANSPARENCY_REPORTING_ENABLED=false
