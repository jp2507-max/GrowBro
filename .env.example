# Example Environment Variables
APP_ENV=development

# API Configuration
API_URL=https://api-dev.example.com
VAR_NUMBER=123
VAR_BOOL=true

# Strains API Configuration
# Direct API access (development only)
STRAINS_API_URL=https://the-weed-db.p.rapidapi.com
STRAINS_API_KEY=your_rapidapi_key_here
STRAINS_API_HOST=the-weed-db.p.rapidapi.com

# Proxy Configuration
# Set to true to use Supabase Edge Function proxy (recommended for production)
STRAINS_USE_PROXY=false

# Feature Flags for Strains
FEATURE_STRAINS_ENABLED=true
FEATURE_STRAINS_FAVORITES_SYNC=true
FEATURE_STRAINS_OFFLINE_CACHE=true

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Sentry Configuration
SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0
SENTRY_SEND_DEFAULT_PII=false
SENTRY_REPLAYS_SESSION_SAMPLE_RATE=0.1
SENTRY_REPLAYS_ON_ERROR_SAMPLE_RATE=1.0
SENTRY_ENABLE_REPLAY=true

# Build-time variables
SECRET_KEY=replace_me
EXPO_PUBLIC_ACCOUNT_DELETION_URL=https://growbro.app/delete-account
